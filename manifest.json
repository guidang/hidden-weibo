{
  "manifest_version": 2,
  "name": "微博隐藏",
  "version": "1.0",
  "description": "新浪微博批量设置隐藏工具",
  "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
  "content_scripts": [
      {
          "matches": ["*://weibo.com/p/*"],
          "js": [
              "js/jquery-3.1.1.min.js",
              "js/common.js"
          ],
          "css": [
              "css/common.css"
          ]
      }
  ]
}